getgenv().buyegg = false
getgenv().walkSpeed = false
getgenv().autoCollect = false
getgenv().autoDeposit = false
getgenv().autoMonkey1 = false
getgenv().autoMonkey5 = false
getgenv().autoMerge = false
getgenv().autoRate = false
getgenv().SecureMode = true
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/UI-Interface/CustomFIeld/main/RayField.lua'))()


local Window = Rayfield:CreateWindow({
	Name = "Monkey Tycoon by Lol123#0030",
	LoadingTitle = "Monkey Tycoon",
	LoadingSubtitle = "Lol123#0030",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = Farming, -- Create a custom folder for your hub/game
		FileName = ""
	},
        Discord = {
        	Enabled = false,
        	Invite = "", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	
})
local Tab = Window:CreateTab("Main", 4483362458)
local Section = Tab:CreateSection("Farming")
local Toggle = Tab:CreateToggle({
	Name = "Auto Collect",
	CurrentValue = false,
	Flag = "Auto Collect", 
	Callback = function(Value)
		getgenv().autoCollect = Value
        if Value then
        autoCollect()
	end
end
})

function autoCollect()
    local itemIds = {1, 6, 36, 162, 810, 2916, 10206, 34992, 118098, 433026, 1417176}
    spawn(function()
        while getgenv().autoCollect == true do
            for _, itemId in pairs(itemIds) do
                local args = {
                    [1] = itemId
                }
                game:GetService("ReplicatedStorage").GTycoonClient.Remotes.GrabDrops:FireServer(unpack(args))
            end
            wait(5)
        end 
    end)
end



local Toggle = Tab:CreateToggle({
	Name = "Auto Deposit",
	CurrentValue = false,
	Flag = "Auto Deposit", 
	Callback = function(Value)
		getgenv().autoDeposit = Value
        if Value then
            autoDeposit()
	end
end
})

function autoDeposit()
    spawn(function()
        while getgenv().autoDeposit == true do
            game:GetService("ReplicatedStorage").GTycoonClient.Remotes.DepositDrops:FireServer()
            wait()
        end 
    end)
end

local Toggle = Tab:CreateToggle({
	Name = "Auto Rate",
	CurrentValue = false,
	Flag = "Auto Rate", 
	Callback = function(Value)
		getgenv().autoRate = Value
        if Value then
            autoRate()
	end
end
})

function autoRate()
    spawn(function()
        while getgenv().autoRate == true do
            local args = {
                [1] = 1
            }
            
            game:GetService("ReplicatedStorage").GTycoonClient.Remotes.BuySpeed:FireServer(unpack(args))
            wait()
        end 
    end)
end

local Tab = Window:CreateTab("Monkey Stuff", 4483362458)
local Section = Tab:CreateSection("Monkeys")
local Toggle = Tab:CreateToggle({
	Name = "Auto Merge",
	CurrentValue = false,
	Flag = "Auto Merge", 
	Callback = function(Value)
		getgenv().autoMerge = Value
        if Value then
            autoMerge()
	end
end
})

function autoMerge()
    spawn(function()
        while getgenv().autoMerge == true do
            game:GetService("ReplicatedStorage").GTycoonClient.Remotes.MergeDroppers:FireServer()
            wait()
        end 
    end)
end


local Toggle = Tab:CreateToggle({
	Name = "Auto Buy Monkeys +1",
	CurrentValue = false,
	Flag = "Auto Monkey", 
	Callback = function(Value)
		getgenv().autoMonkey1 = Value
        if Value then
            autoMonkey1()
	end
end
})

